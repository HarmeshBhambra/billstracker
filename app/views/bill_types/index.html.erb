<div class="page-header">
  <h1>
    Bill types
    <a href="/bill_types/new" class="btn btn-lg btn-success">New Bill type</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Actions</th>
      </tr>

      <% @bill_types.each do |bill_type| %>
      <tr>
        <td>
          <a><%= bill_type.type %>
        <td>
          <a href="/bill_types/<%= bill_type.id %>" class="btn btn-primary">Show</a>
          <a href="/bill_types/<%= bill_type.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_bill_type/<%= bill_type.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
